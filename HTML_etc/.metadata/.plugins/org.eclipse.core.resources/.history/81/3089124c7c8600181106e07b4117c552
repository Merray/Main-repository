
function bonjour(){
	
	console.log("Hello");
}



function changePlayer(joueur){
	
	if(joueur == 1){
		joueur = 2;
	}
	else{
		joueur = 1;
	}
	
	return joueur;
}

function morpion(){
	
	var player = 2;
	var win = 1;
	
	while (win == 1) {
	    
		player = changePlayer(player);
		
		$(".afficheJoueur").html("Joueur :"+player);		
	}
	
}